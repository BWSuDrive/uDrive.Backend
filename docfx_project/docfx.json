{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/*/*.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "includePrivateMembers": true,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "separatePages",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "disableGitFeatures": false,
    "fileMetadataFiles": [],
    "globalMetadataFiles": {
      "_appTitle": "uDrive.Backend",
      "_appLogoPath": "/images/udrive-low-resolution-logo-color-on-transparent-background.png"
    },
    "keepFileLink": false,
    "output": "_site",
    "postProcessors": [],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "template": [
      "default",
      "modern"
    ]
  }
}